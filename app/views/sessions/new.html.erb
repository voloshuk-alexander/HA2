<% provide(:title, 'Sign in') %>
<h1>Sign in</h1>

<div class="row">
  <div class="span6 offset3">
    <%= simple_form_for :user, url: users_path do |f| %>



      <%= f.input :email %>


      <%= f.input :password %>




      <%= f.button :submit, "Sign in", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
